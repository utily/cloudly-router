{
	"compilerOptions": {
		"target": "es6",
		"module": "es6",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"outDir": "dist",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"lib": ["esnext", "webworker"],
		"alwaysStrict": true,
		"strict": true,
		"esModuleInterop": true,
		"allowJs": true
	},
	
	"typeAcquisition": {
		"enable": true
	}
}
